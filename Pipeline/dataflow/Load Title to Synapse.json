{
	"name": "Load Title to Synapse",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "adls_title",
						"type": "DatasetReference"
					},
					"name": "adlstitle"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "synapse_title",
						"type": "DatasetReference"
					},
					"name": "synapsetitle"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          TitleCode as string,",
				"          TitleDescription as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> adlstitle",
				"adlstitle sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          TitleCode as string,",
				"          TitleDescription as string",
				"     ),",
				"     filePattern:'Title.csv',",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          TitleCode,",
				"          TitleDescription",
				"     )) ~> synapsetitle"
			]
		}
	}
}